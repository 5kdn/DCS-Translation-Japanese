    ["name"] = "Su-27 - Fortress Mozdok",
    ["name_CN"] = "Su-27 - 莫兹多克要塞",
    ["name_JP"] = "Su-27 - モズドクの要塞(Fortress Mozdok)",
    ["name_CS"] = "Su-27 - Pevnost Mozdok",
    ["name_DE"] = "Su-27 - Festung Mozdok",
    ["name_ES"] = "Su-27 - La fortaleza de Mozdok",
    ["name_KO"] = "Su-27 - 모즈독 요새 (Fortress Mozdok)",
    ["name_RU"] = "Су-27 - Крепость Моздок",
    
    ["picture"] = "SU27camp_title.png",
    ["picture_CN"] = "SU27camp_title_cn.png",
